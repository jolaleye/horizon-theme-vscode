  "tokenColors": [
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "foreground": "#D5D8DA66",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": ["string.quoted", "string.template", "punctuation.definition.string"],
      "settings": {
        "foreground": "#FAB795E6"
      }
    },
    {
      "name": "Variable",
      "scope": "variable",
      "settings": {
        "foreground": "#E95678E6"
      }
    },
    {
      "name": "Language variable",
      "scope": "variable.language",
      "settings": {
        "foreground": "#FAC29AE6",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Parameter",
      "scope": "variable.parameter",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Storage (declaration or modifier keyword)",
      "scope": ["storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#B877DBE6",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Constant",
      "scope": "constant",
      "settings": {
        "foreground": "#F09483E6"
      }
    },
    {
      "name": "Regex",
      "scope": "string.regexp",
      "settings": {
        "foreground": "#F09483E6"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#F09483E6"
      }
    },
    {
      "name": "Language constant (boolean, null)",
      "scope": "constant.language",
      "settings": {
        "foreground": "#F09483E6"
      }
    },
    {
      "name": "Character escape",
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#25B0BCE6"
      }
    },
    {
      "name": "Entity (data interfaces)",
      "scope": "entity.name",
      "settings": {
        "foreground": "#FAC29AE6"
      }
    },
    {
      "name": "HTML or XML tag",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#E95678E6"
      }
    },
    {
      "name": "HTML or XML tag brackets",
      "scope": ["punctuation.definition.tag.begin", "punctuation.definition.tag.end"],
      "settings": {
        "foreground": "#E95678A6"
      }
    },
    {
      "name": "Tag atttribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#F09483E6",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Class",
      "scope": "entity.name.type",
      "settings": {
        "foreground": "#FAC29AE6"
      }
    },
    {
      "name": "Inherited class",
      "scope": "entity.other.inherited-class",
      "settings": {
        "foreground": "#FAB795E6"
      }
    },
    {
      "name": "Function",
      "scope": ["entity.name.function", "variable.function"],
      "settings": {
        "foreground": "#25B0BCE6"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword",
      "settings": {
        "foreground": "#B877DBE6"
      }
    },
    {
      "name": "Control keyword (if, try, while, etc.)",
      "scope": "keyword.control",
      "settings": {
        "foreground": "#B877DBE6",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Operator",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#BBBBBB"
      }
    },
    {
      "name": "Special operator",
      "scope": ["keyword.operator.new", "keyword.operator.expression", "keyword.operator.logical"],
      "settings": {
        "foreground": "#B877DBE6"
      }
    },
    {
      "name": "Unit",
      "scope": "keyword.other.unit",
      "settings": {
        "foreground": "#F09483E6"
      }
    },
    {
      "name": "Support",
      "scope": "support",
      "settings": {
        "foreground": "#FAC29AE6"
      }
    },
    {
      "name": "Support function",
      "scope": "support.function",
      "settings": {
        "foreground": "#25B0BCE6"
      }
    },
    {
      "name": "Support variable",
      "scope": "support.variable",
      "settings": {
        "foreground": "#E95678E6"
      }
    },
    {
      "name": "Object literal key / property",
      "scope": ["meta.object-literal.key", "support.type.property-name"],
      "settings": {
        "foreground": "#E95678E6"
      }
    },
    {
      "name": "Key-value separator",
